<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../layouts/head') %>
    </head>
    <header>
        <%- include('../layouts/header') %>
    </header>
    <main id="sortable">

        <% output.forEach(object=>{ %>
            <section class="ui-state-default">
                <div>
                        <form method = "post" action = "/details">

                            <input type="hidden" name= <%= output.symbol %>
                            <button type = "submit"> <%= object.companyname %> </br> Ticker symbol: <%= object.symbol %> </button>
                    
                        </form>
                </div>
            </section>
        <% }) %>

    </main>
    <footer>
        <%- include('../layouts/footer') %>
    </footer>
</html>